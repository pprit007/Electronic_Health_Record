<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.ehr">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OptionMain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DoctorLogin"
            android:label="@string/Doctor" />
        <activity
            android:name=".PatientLogin"
            android:label="@string/Patient" />
        <activity
            android:name=".p_navigation"
            android:label="@string/title_activity_p_navigation"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>